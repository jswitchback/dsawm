.img-shadow { 
    @include box-shadow(0 1px 4px rgba(0, 0, 0, 0.2), 0 0 40px rgba(0, 0, 0, 0.1) inset);
    position: relative; 
    background-color: #FFFFFF; 
    border-width: 5px; 
    border-style: solid; 
    border-color: #ffffff; 
    border-radius: 0px; 
    &:before {
      content: ""; 
      z-index: -1; 
      position: absolute; 
      width: 50%; 
      top: 160px; 
      bottom: 15px; 
      left: 10px; 
      -webkit-transform: rotate(-3deg); 
      transform: rotate(-3deg); 
      box-shadow: 0px 15px 10px #777; 
    }
    &:after {
      content: ""; 
      z-index: -1; 
      position: absolute; 
      width: 50%; 
      top: 160px; 
      right: 10px; 
      bottom: 15px; 
      -webkit-transform: rotate(3deg); 
      transform: rotate(3deg); 
      box-shadow: 0px 15px 10px #777; 
    }
}

.no-boxshadow {
  .img-shadow {
    border: 5px solid #d1d1d1;
  }
