  // Add both horizontal and baseline grids to an element's background
}

/* Typography
   ========================================================================== */

.field-resource-link a,
[class*="icon-"] {
  &:before {
  font-family: 'icomoon';
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  vertical-align: middle;
  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  }
}

#block-views-promotions-block-1 .views-field-title a,
#block-views-promotions-block .views-field-title a,
.field-resource-link a {
  display: inline-block;
  &:after {
  font-family: 'icomoon';
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1.2;
  vertical-align: middle;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  }
  &:hover {
    &:after {
      padding-right: 6px;
    }
  }
}

